    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>JSON, serialization</PackageTags>
    <Version>1.0.3</Version>
    <AssemblyVersion>1.0.3.0</AssemblyVersion>
  </PropertyGroup>

</Project>
